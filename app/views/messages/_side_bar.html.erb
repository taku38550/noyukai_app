<div class="side_bar_header">
  <div class="header_name">
    <%= link_to current_user.nickname, root_path %>
  </div>
  <div class="create_room">
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>

<div class="rooms">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
        <%= link_to room.name, root_path %>
      </div>
    </div>
  <% end %>
</div>